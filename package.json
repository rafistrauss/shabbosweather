{
  "name": "shabbos-weather",
  "description": "Weather display for shabbos",
  "version": "0.0.5",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "npm run clean && sapper export --basepath shabbosweather",
    "myexportserver": "npm run clean && sapper export --basepath shabbosweather && npx serve __sapper__/export/",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "clean": "rm -rf ./__sapper__",
    "test": "run-p --race dev cy:run",
    "deploy": "npm run export && mv __sapper__/export/shabbosweather/* __sapper__/export/ && node ./scripts/deploy.js"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "date-fns": "^2.13.0",
    "gh-pages": "^3.0.0",
    "node-fetch": "^2.6.0",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "pretty-quick": "^2.0.1",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^6.1.0",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
